{
    "name": "portfolio",
    "version": "1.0.0",
    "private": true,
    "description": "Portfolio",
    "author": "Mikecheek",
    "homepage": "https://mikecheek.github.io/portfolio",
    "keywords": [
        "portfolio"
    ],
    "license": "UNLICENSED",
    "scripts": {
        "develop": "gatsby develop",
        "start": "gatsby develop",
        "build": "gatsby build",
        "serve": "gatsby serve",
        "clean": "gatsby clean",
        "deploy": "gatsby build && gh-pages -d public",
        "deploygh": "gatsby build --prefix-paths && gh-pages -d public",
        "push": "git add . && git commit && git push",
        "prepare": "husky install",
        "format": "prettier --write .",
        "lint-init": "eslint --init",
        "prettify": "pretty-quick",
        "type-check": "tsc",
        "check-types": "tsc --pretty --noEmit",
        "check-format": "prettier --check .",
        "test-all": "npm run check-format && npm run check-lint && npm run check-types && npm run build"
    },
    "dependencies": {
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "gatsby": "^4.3.0",
        "gatsby-plugin-google-gtag": "^4.15.0",
        "gatsby-plugin-image": "^2.3.0",
        "gatsby-plugin-manifest": "^4.4.0",
        "gatsby-plugin-offline": "^5.14.0",
        "gatsby-plugin-react-helmet": "^5.4.0",
        "gatsby-plugin-react-svg": "^3.1.0",
        "gatsby-plugin-robots-txt": "^1.7.1",
        "gatsby-plugin-sass": "^5.8.0",
        "gatsby-plugin-sharp": "^4.3.0",
        "gatsby-plugin-sitemap": "^5.3.0",
        "gatsby-plugin-typescript": "^4.3.0",
        "gatsby-source-filesystem": "^4.3.0",
        "gatsby-transformer-sharp": "^4.3.0",
        "gh-pages": "^3.2.3",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-ga4": "^1.4.1",
        "react-helmet": "^6.1.0",
        "react-intersection-observer": "^9.1.0",
        "sass": "^1.49.9",
        "stable": "^0.1.8"
    },
    "devDependencies": {
        "@types/node": "^16.11.12",
        "@types/react": "^17.0.37",
        "@types/react-dom": "^17.0.11",
        "@types/react-helmet": "^6.1.4",
        "@types/styled-components": "^5.1.19",
        "@typescript-eslint/eslint-plugin": "^5.17.0",
        "@typescript-eslint/parser": "^5.17.0",
        "eslint": "^8.12.0",
        "eslint-plugin-react": "^7.29.4",
        "husky": "^7.0.0",
        "prettier": "2.6.2",
        "pretty-quick": "^3.1.3",
        "uuid": "^8.3.2"
    },
    "peerDependencies": {
        "@babel/core": "^7.12.3",
        "babel-eslint": "^10.1.0",
        "typescript": "^4.5.5",
        "webpack": "^5.0.0"
    }
